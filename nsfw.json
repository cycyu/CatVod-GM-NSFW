{
  "spider": "./lib/GM.jar",
  "sites": [
    {
      "key": "Jable",
      "name": "Jable",
      "type": 3,
      "searchable": 1,
      "changeable": 0,
      "style": {
        "type": "rect",
        "ratio": 1.48
      },
      "api": "csp_GM",
      "ext": {
        "userScript": "./js/jable.user.js",
        "debug": false,
        "webViewSettings": {
          "blockNetworkImage": true,
          "blockList": [
            "*.ico",
            "*.png",
            "https://*.adsco.re/*",
            "https://*.googleapis.com/*",
            "https://*.googlesyndication.com/*",
            "https://*.googletagmanager.com/*",
            "https://*.hpyjmp.com/*",
            "https://*.magsrv.com/*",
            "https://*.mnaspm.com/*",
            "https://*.shukriya90.com/*",
            "https://*.tapioni.com/*",
            "https://*.tsyndicate.com/*",
            "https://assets.jable.tv/assets/js/player.js"
          ]
        },
        "spider": {
          "homeContent": {
            "loadUrl": "https://jable.tv"
          },
          "categoryContent": {
            "loadUrl": "https://jable.tv/${tid:-latest-updates}/?from=${pg:-1}${sort_by:-}"
          },
          "detailContent": {
            "loadUrl": "https://jable.tv/videos/${id}/"
          },
          "searchContent": {
            "loadUrl": "https://jable.tv/search/${key}/?from=${pg:-1}"
          }
        }
      }
    },
    {
      "key": "nJAV",
      "name": "nJAV",
      "type": 3,
      "searchable": 1,
      "changeable": 0,
      "style": {
        "type": "rect",
        "ratio": 1.49
      },
      "api": "csp_GM",
      "ext": {
        "userScript": "./js/njav.user.js",
        "debug": false,
        "webViewSettings": {
          "blockNetworkImage": true,
          "blockList": [
            "*.ico",
            "*.png",
            "*.jpg",
            "*.vtt",
            "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css",
            "https://fonts.googleapis.com/*",
            "https://www.googletagmanager.com/*",
            "https://*.recombee.com/*",
            "https://*.google-analytics.com/*",
            "https://javplayer.me/assets/media/dist/app.js*"
          ]
        },
        "spider": {
          "homeContent": {
            "loadUrl": "https://njav.tv/zh/"
          },
          "categoryContent": {
            "loadUrl": "https://njav.tv/zh/${tid}?page=${pg:-1}${filter:-}${sort:-}"
          },
          "detailContent": {
            "loadUrl": "https://njav.tv/zh/v/${id}"
          },
          "playerContent": {
            "loadUrl": "${id}"
          },
          "searchContent": {
            "loadUrl": "https://njav.tv/zh/search?keyword=${key}&page=${pg:-1}"
          }
        }
      }
    }
  ]
}